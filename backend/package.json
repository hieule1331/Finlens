{
  "name": "finlens-backend",
  "version": "1.0.0",
  "description": "Vietnamese Stock Market Analysis Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "ts-node src/database/migrate.ts",
    "seed": "ts-node src/database/seed.ts",
    "db:verify": "ts-node src/database/verify.ts",
    "test:db": "ts-node src/test-db.ts",
    "test:api": "ts-node src/test/api.test.ts",
    "test": "npm run test:api",
    "setup": "docker compose up -d && npm run migrate && npm run seed",
    "verify": "ts-node src/scripts/verify-system.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "stock",
    "market",
    "vietnam",
    "analysis"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^8.15.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
